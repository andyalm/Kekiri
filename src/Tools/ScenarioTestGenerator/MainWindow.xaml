<Window x:Class="Kekiri.ScenarioTestGenerator.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="Scenario Test Generator" Height="350" Width="525" WindowStartupLocation="CenterScreen">
    <DockPanel>
        <StackPanel DockPanel.Dock="Bottom" Orientation="Horizontal">
            <Button Click="OnGenerateClick">
                Generate
            </Button>
        </StackPanel>
        <TextBox x:Name="_textBox" TextWrapping="Wrap" AcceptsReturn="True" />
    </DockPanel>
</Window>
